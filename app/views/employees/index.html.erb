<h1>employees</h1>

<% if @employees.any? %>
  <ul>
    <% @employees.each do |employee| %>
      <li><%= link_to employee.name, employee_path(employee) %></li>
    <% end %>
  </ul>
<% else %>
  <p>you have no employees </p>
<% end %>


<%= button_to "Create new employee", 
              new_employee_path,
              method: :get %>